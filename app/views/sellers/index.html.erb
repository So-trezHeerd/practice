<p id="notice"><%= notice %></p>

<h1>Sellers</h1>

<table>
  <thead>
    <tr>
      <th>메이커</th>
      <th>User</th>
      <th>사업자 등록번호</th>
      <th>상호/법인명</th>
      <th>대표자명</th>
      <th>소재지</th>
      <th>대표 이메일</th>
      <th>대표 번호</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sellers.each do |seller| %>
      <tr>
        <td><%= seller.seller_id %></td>
        <td><%= seller.user %></td>
        <td><%= seller.registration_num %></td>
        <td><%= seller.company_name %></td>
        <td><%= seller.ceo_name %></td>
        <td><%= seller.company_address %></td>
        <td><%= seller.ceo_email %></td>
        <td><%= seller.ceo_num %></td>
		
        <td><%= link_to 'Show', seller %></td>
        <td><%= link_to 'Edit', edit_seller_path(seller) %></td>
        <td><%= link_to 'Destroy', seller, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seller', new_seller_path %>
